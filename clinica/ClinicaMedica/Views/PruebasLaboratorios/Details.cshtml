@model ClinicaMedica.Data.Models.PruebasLaboratorio

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Prueba de Laboratorio</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TipoPrueba)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoPrueba)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaPrueba)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaPrueba)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Resultado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Resultado)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Laboratorio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Laboratorio.LaboratorioId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Paciente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Paciente.PacienteId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.PruebaId" class="btn btn-sm btn-warning">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
